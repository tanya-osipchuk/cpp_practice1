#include <iostream>
#include <string>
using namespace std;

int main() {
   system("chcp 65001");
   string s = "Яблок";
   string end = "о";
   string end1 = "а";

    for (int num = 0; num < 100; num++) {
        if (num == 1 || (num > 20 && num % 10 == 1))
            cout << num << " " << s << end << endl;
        else if ( (num > 1 && num < 5) || ((num > 20) && (num % 10) > 1 && (num % 10) < 5))
            cout << num << " " << s << end1 << endl;
        else
            cout << num << " " << s << endl;
    }
    return 0;
}
